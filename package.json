    "targetPlatforms": [
      "aplite",
      "diorite"
    ],
    "companyName": "Devin Bernosky",
    "versionLabel": "1.0.0",
    "shortName": "FitzFace",
    "longName": "FitzFace - Weather, MUNI, Tides & Pollen",
    "watchapp": {
      "watchface": true
    },
    "capabilities": [
