{
  "name": "fitzface",
  "author": "Devin Bernosky",
  "version": "1.0.0",
  "description": "High-density data-rich watchface featuring weather, MUNI bus tracking, tides, air quality, pollen levels, and smart health alerts for Pebble smartwatches.",
  "keywords": ["pebble-app"],
  "private": true,
  "dependencies": {
    "pebble-clay": "^1.0.4"
  },
  "pebble": {
